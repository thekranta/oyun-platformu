{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
{
                "version": 2,
                "builds": [
                    {
                        "src": "package.json",
                        "use": "@vercel/static-build",
                        "config": {
                            "distDir": "dist"
                        }
                    },
                    {
                        "src": "api/**/*.ts",
                        "use": "@vercel/node"
                    }
                ],
                "routes": [
                    {
                        "handle": "filesystem"
                    },
                    {
                        "src": "/api/(.*)",
                        "dest": "/api/$1"
                    },
                    {
                        "src": "/((?!api/.*).*)",
                        "dest": "/index.html"
                    }
                ]
            }